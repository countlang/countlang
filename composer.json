{
  "name": "countlang/countlang",
  "description": "PHP library for the Unicode CLDR project",
  "type": "library",
  "keywords": ["country", "language", "currency", "region", "cldr", "iso-3166", "iso-639", "iso-4217"],
  "license": "MIT",
  "authors": [
    {
      "name": "Andrii Bibena",
      "email": "countlang@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "phpdocumentor/reflection-docblock": "3.1.*",
    "doctrine/annotations": "1.4.*",
    "doctrine/cache": "1.6.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0"
  },
  "autoload": {
    "psr-4": {
      "CountLang\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CountLang\\Tests\\": "tests/"
    }
  }
}